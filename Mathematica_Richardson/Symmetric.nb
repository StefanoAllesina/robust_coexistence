Cell[BoxData[
 RowBox[{
  RowBox[{"BuildB", "[", "n_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"A", ",", " ", "B"}], "}"}], ",", 
    RowBox[{
     RowBox[{"A", " ", "=", " ", 
      RowBox[{"UpperTriangularize", "[", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"b", "[", 
           RowBox[{"i", ",", "j"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", "n"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "1", ",", "n"}], "}"}]}], "]"}], ",", "1"}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"B", "=", 
      RowBox[{"A", "+", 
       RowBox[{"Transpose", "[", "A", "]"}]}]}], ";", "\[IndentingNewLine]", 
     "B"}]}], "]"}]}]], "Input",
