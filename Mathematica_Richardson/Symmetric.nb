   RowBox[{
    RowBox[{"{", 
     RowBox[{"B", ",", " ", "ones", ",", " ", "m", ",", " ", "K"}], "}"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"B", "=", 
      RowBox[{"BuildB", "[", "n", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"ones", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"1", ",", "n"}], "]"}]}], ";", 
